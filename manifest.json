{
    "manifest_version":2,

    "name":"Highlight Practice",
    "description": "this extension is for highting the world",
    "version":"1.0",
    "icons":{
        "128":"icon_128.png",
        "48":"icon_48.png",
        "16":"icon_16.png"
    },

    "browser_action":{
        "default_icon":"icon_16.png",
        "default_popup":"popup.html"
    },

    "permissions": [
        "storage",
        "tabs",
        "*://*.hololink.co/",
        "cookies"
    ],

    "content_scripts":[
        {
            "matches":["http://*/*"],
            "js":["getHtml.js"]
        }   
    ]
}